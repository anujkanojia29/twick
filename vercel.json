{
  "version": 2,
  "buildCommand": "pnpm install && pnpm run build",
  "outputDirectory": "packages/examples/dist",
  "installCommand": "pnpm install",
  "framework": "vite",
  "ignoreCommand": "git diff HEAD^ HEAD --quiet ./packages/examples/",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "commands": {
    "build": "pnpm install && pnpm run build",
    "start": "cd packages/examples && pnpm run preview"
  }
}